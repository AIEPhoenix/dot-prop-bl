{
	"name": "dot-prop-bl",
	"version": "8.0.8",
	"description": "Get, set, or delete a property from a nested object using a dot path",
	"license": "MIT",
	"repository": "AIEPhoenix/dot-prop-bl",
	"funding": "https://github.com/AIEPhoenix/dot-prop-bl",
	"author": {
		"name": "Brian Lee",
		"email": "brianlee93@vip.qq.com"
	},
	"main": "dist/index.cjs.js",
	"module": "dist/index.esm.js",
	"umd:main": "dist/index.umd.js",
	"types": "index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.esm.js",
			"require": "./dist/index.cjs.js",
			"default": "./dist/index.umd.js"
		},
		"./dist/index.esm.js": "./dist/index.esm.js",
		"./dist/index.umd.js": "./dist/index.umd.js",
		"./dist/index.cjs.js": "./dist/index.cjs.js"
	},
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "vite build",
		"test": "ava",
		"bench": "node benchmark.js"
	},
	"files": [
		"index.js",
		"dist",
		"index.d.ts"
	],
	"keywords": [
		"object",
		"prop",
		"property",
		"dot",
		"path",
		"get",
		"set",
		"delete",
		"access",
		"notation",
		"dotty"
	],
	"devDependencies": {
		"ava": "^5.2.0",
		"benchmark": "^2.1.4",
		"typescript": "^5.0.4",
		"vite": "^4.4.9",
		"xo": "^0.54.1"
	}
}
